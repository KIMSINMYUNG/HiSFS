@page "/test/DGridTest"

<DGrid DataSource=@(list) TValue="Info" DefaultMode="true" EditMode="DGridEditMode.All">
    <GridColumns>
        <GridColumn Field=@nameof(Info.A) IsPrimaryKey="true" />
        <GridColumn Field=@nameof(Info.B) />
        <GridColumn Field=@nameof(Info.C) />
        <GridColumn Field=@nameof(Info.D) />
        <GridColumn Field=@nameof(Info.E) />
    </GridColumns> 
</DGrid>

@code {
    private ObservableCollection<Info> list = new ObservableCollection<Info>
    {
        new Info { A = "1", B = "2", C = "3", D = "4", E = "5" },
        new Info { A = "2", B = "2", C = "3", D = "4", E = "5" },
        new Info { A = "3", B = "2", C = "3", D = "4", E = "5" },
        new Info { A = "4", B = "2", C = "3", D = "4", E = "5" },
        new Info { A = "5", B = "2", C = "3", D = "4", E = "5" }
    };

    public class Info
    {
        public string A { get; set; }
        public string B { get; set; }
        public string C { get; set; }
        public string D { get; set; }
        public string E { get; set; }
    }
}
